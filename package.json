{
  "name": "click-clack",
  "version": "0.0.5",
  "description": "A simple typing game",
  "main": "index.js",
  "scripts": {
    "serve": "./node_modules/live-server/live-server.js .",
    "server": "./node_modules/live-server/live-server.js .",
    "lint": "./node_modules/eslint/bin/eslint.js js/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonase/click-clack.git"
  },
  "keywords": [
    "typing",
    "game",
    "practice",
    "test"
  ],
  "author": "James Dixon",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/lemonase/click-clack/issues"
  },
  "homepage": "https://github.com/lemonase/click-clack#readme",
  "devDependencies": {
    "eslint": "^7.3.1",
    "jshint": "^2.11.1",
    "live-server": "^1.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
